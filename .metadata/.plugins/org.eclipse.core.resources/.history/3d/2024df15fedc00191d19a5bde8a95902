package Testngsamples;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class GmailSend {
	
	public static void main(String[] args) throws Exception {
	
		System.setProperty("webdriver.chrome.driver", "F:\\software\\chromedriver_win32 (1)\\chromedriver.exe");
		WebDriver driver=new ChromeDriver();
		driver.get("https://mail.google.com");
		driver.findElement( By.id("identifierId")).sendKeys("sabarish547");
		driver.findElement(By.xpath("//span[text()='Next']")).click();
		Thread.sleep(3000);
		driver.findElement(By.name("password")).sendKeys("sabi290394");
		driver.findElement(By.xpath("//span[text()='Next']")).click();

	}

}
